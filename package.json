		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"prepare": "husky install"
	},
	"devDependencies": {
		"@playwright/test": "^1.41.1",
		"@sveltejs/adapter-auto": "^3.1.1",
